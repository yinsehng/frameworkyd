package com.neusoft.controller.business;

import javax.servlet.http.HttpServletRequest;
import javax.servlet.http.HttpServletResponse;

import org.springframework.beans.factory.annotation.Autowired;
import org.springframework.stereotype.Controller;

import com.alibaba.fastjson.JSON;
import com.neusoft.controller.util.SuperAction;
import com.neusoft.service.IAllPersonCountWeightService;
import com.neusoft.util.ResultModel;
@Controller
public class AllPersonCountWeightAction implements SuperAction{

	@Autowired
	private IAllPersonCountWeightService allPersonCountWeightService;
	@Override
	public String actionAjaxHandler(HttpServletRequest request,
			HttpServletResponse response) {
		ResultModel allPersonCountWeight_rm = allPersonCountWeightService.getAllPersonCountWeight();
		String allPersonCountWeight_json = JSON.toJSONString(allPersonCountWeight_rm);
		return allPersonCountWeight_json;
	}

	@Override
	public void actionForwardHandler(HttpServletRequest request,
			HttpServletResponse response) {
		// TODO Auto-generated method stub
		
	}

}
